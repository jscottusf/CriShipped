<div class="catalogue-container">
{% if messages.success %}
    <div class="alert alert-primary" role="alert"><i class="fas fa-shopping-cart"></i> {{messages.success}}</div>
  {% endif %}
  <div class="container">
    <div class="row">
      <div class="col-md-3 index__filter-wrapper">
        <h4 class="index__filter-title">Filter By</h4>
        <hr />
        <div>
          {% include 'partials/category.njk' %}
        </div>
        <div>
          {% include 'partials/brand.njk' %}
        </div>
        <div class="py-2">
        <a href="/catalogue"><button class="btn btn-primary">Refresh Catalogue</button></a>
        </div>
      </div>
      
      {# /catalogue/ #}
      
      <div class="col-md-9 {% if page_object %}index__product-wrapper--has-pages{%else%}index__product-wrapper--no-pages{% endif%}">
        {% include 'partials/product_list.njk' %}
      </div>
    </div>
  </div>
</div>
